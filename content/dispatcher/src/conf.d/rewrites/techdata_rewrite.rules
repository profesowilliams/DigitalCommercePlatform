# redirect root
RedirectMatch "^/$" "/us/en.html"

# robot.txt
RewriteRule ^/robots.txt$ /content/dam/${CONTENT_FOLDER_NAME_TECHDATA}/seo/robots.txt [PT,L]

RewriteRule ^/(ar|br|ca|cl|co|ec|mx|pe|us)/sitemap.xml$ /content/${CONTENT_FOLDER_NAME_TECHDATA}/americas/$1/en.sitemap.xml [PT,L]
RewriteRule ^/(apac|au|hk|hk_tdcs|id|in|my|nz|sg|th|vn)/sitemap.xml(.*)$ /content/${CONTENT_FOLDER_NAME_TECHDATA}/apac/$1/en.sitemap.xml [PT,L]
RewriteRule ^/(at|be|ch|cs|cz|de|dk|es|fi|fr|hr|hu|ie|it|nl|no|pl|pt|ro|se|sk|tr|uk)/sitemap.xml(.*)$ /content/${CONTENT_FOLDER_NAME_TECHDATA}/europe/$1/en.sitemap.xml [PT,L]

RewriteCond %{REQUEST_URI} !(/|.jpe?g|.html|.svg|.json|.png|.txt|.xml|.cache)$

# apac
RewriteRule ^/(jp/ja)$ /content/${CONTENT_FOLDER_NAME_TECHDATA}/apac/$1.html [PT,L,NC]
RewriteRule ^/(apac|au|hk|hk_tdcs|id|in|my|nz|sg|th|vn)$ /content/${CONTENT_FOLDER_NAME_TECHDATA}/apac/$1/en.html [PT,L,NC]

# americas
RewriteRule ^/(mx/en|br/en|ca/fr)$ /content/${CONTENT_FOLDER_NAME_TECHDATA}/americas/$1.html [PT,L,NC]
RewriteRule ^/(ar|ca|cl|co|ec|pe|us)$ /content/${CONTENT_FOLDER_NAME_TECHDATA}/americas/$1/en.html [PT,L,NC]
RewriteRule ^/(br|mx)$ /content/${CONTENT_FOLDER_NAME_TECHDATA}/americas/$1/$1.html [PT,L,NC]

# europe
RewriteRule ^/(cz/en|fr/en|it/en)$ /content/${CONTENT_FOLDER_NAME_TECHDATA}/europe/$1.html [PT,L,NC]
RewriteRule ^/(at|be|ch|cs|de|dk|es|fi|hr|hu|ie|nl|no|pl|pt|ro|se|sk|tr|uk)$ /content/${CONTENT_FOLDER_NAME_TECHDATA}/europe/$1/en.html [PT,L,NC]
RewriteRule ^/(cz|fr|it)$ /content/${CONTENT_FOLDER_NAME_TECHDATA}/europe/$1/$1.html [PT,L,NC]

RewriteCond %{REQUEST_URI} !^/apps
RewriteCond %{REQUEST_URI} !^/bin
