# robot.txt
RewriteRule ^/robots.txt$ /content/dam/techdata/seo/apac/my/robots.txt  [PT,L]
RewriteRule ^/sitemap.xml$ /content/techdata/apac/my/en.sitemap.xml [PT,L]
RewriteCond %{HTTP_HOST} =mytecd.com [NC]
RewriteRule ^/$ /content/${CONTENT_FOLDER_NAME_TECHDATA}/apac/my/en.html [PT,L]
RewriteCond %{HTTP_HOST} =uat.dc.mytecd.com [NC]
RewriteRule ^/$ /content/${CONTENT_FOLDER_NAME_TECHDATA}/apac/my/en.html [PT,L]
RewriteCond %{HTTP_HOST} =stage.dc.mytecd.com [NC]
RewriteRule ^/$ /content/${CONTENT_FOLDER_NAME_TECHDATA}/apac/my/en.html [PT,L]

# Match specific hosts
RewriteCond %{HTTP_HOST} ^(mytecd\.com|uat\.dc\.mytecd\.com|stage\.dc\.mytecd\.com)$ [NC]
# Exclude specific paths
RewriteCond %{REQUEST_URI} !^/(apps|bin|content|etc|home|libs|saml_login|system|tmp|var)
# Match specific file extensions
RewriteCond %{REQUEST_URI} \.(html|jpe?g|png|svg|txt|xml|cache)$ [NC]
# Rewrite the URL
RewriteRule ^/(.*)$ /content/${CONTENT_FOLDER_NAME_TECHDATA}/apac/my/$1 [PT,L]

# Google Site Verification
RewriteRule ^/google(.*).html$ /content/dam/${CONTENT_FOLDER_NAME_TECHDATA}/site-root-files/google$1.html [PT,L]
