<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Properties"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <column
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <tabs
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/tabs"
                        maximized="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <firstStep
                                jcr:primaryType="nt:unstructured"
                                jcr:title="First Step"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <firstStepTitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Start Title"
                                                name="./firstStepTitle"/>
                                            <firstStepBackgroundImage
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="First Step Background Image"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <firstStepBackgroundImage
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                        autoStart="{Boolean}false"
                                                        class="cq-droptarget"
                                                        fileNameParameter="./firstStepBackgroundImageName"
                                                        fileReferenceParameter="./firstStepBackgroundImageReference"
                                                        mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
                                                        multiple="{Boolean}false"
                                                        name="./firstStepBackgroundImage"
                                                        title="First Step Background Image"
                                                        uploadUrl="${suffix.path}"
                                                        useHTML5="{Boolean}true"/>
                                                </items>
                                            </firstStepBackgroundImage>
                                            <firstStepDividerImage
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="First Step Divider Image"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <firstStepDividerImage
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                        autoStart="{Boolean}false"
                                                        class="cq-droptarget"
                                                        fileNameParameter="./firstStepDividerImageName"
                                                        fileReferenceParameter="./firstStepDividerImageReference"
                                                        mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
                                                        multiple="{Boolean}false"
                                                        name="./firstStepDividerImage"
                                                        title="First Step Divider Image"
                                                        uploadUrl="${suffix.path}"
                                                        useHTML5="{Boolean}true"/>
                                                </items>
                                            </firstStepDividerImage>
                                        </items>
                                    </column>
                                </items>
                            </firstStep>
                            <steps
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Steps"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <stepDividerImage
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="Step Divider Image"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <stepDividerImage
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                        autoStart="{Boolean}false"
                                                        class="cq-droptarget"
                                                        fileNameParameter="./stepDividerImageName"
                                                        fileReferenceParameter="./stepDividerImageReference"
                                                        mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
                                                        multiple="{Boolean}false"
                                                        name="./stepDividerImage"
                                                        title="Step Divider Image"
                                                        uploadUrl="${suffix.path}"
                                                        useHTML5="{Boolean}true"/>
                                                </items>
                                            </stepDividerImage>
                                            <steps
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                composite="{Boolean}true"
                                                fieldLabel="Steps">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                                    required="true"
                                                    name="./steps">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <icon
                                                            jcr:primaryType="nt:unstructured"
                                                            jcr:title="Icon"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <icon
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                                    allowUpload="{Boolean}false"
                                                                    autoStart="{Boolean}false"
                                                                    class="cq-droptarget"
                                                                    fileNameParameter="./iconFileName"
                                                                    fileReferenceParameter="./iconFileReference"
                                                                    mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
                                                                    multiple="{Boolean}false"
                                                                    name="./icon"
                                                                    title="Icon"
                                                                    uploadUrl="${suffix.path}"
                                                                    useHTML5="{Boolean}true"/>
                                                            </items>
                                                        </icon>
                                                        <title
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldLabel="Title"
                                                            name="./title"/>
                                                        <description
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                            fieldLabel="Description"
                                                            name="./description"/>
                                                    </items>
                                                </field>
                                            </steps>
                                        </items>
                                    </column>
                                </items>
                            </steps>
                            <lastStep
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Last Step"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <lastStepTitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Last Step Title"
                                                name="./lastStepTitle"/>
                                            <lastStepBackgroundImage
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="Last Step Background Image"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <lastStepBackgroundImage
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                        autoStart="{Boolean}false"
                                                        class="cq-droptarget"
                                                        fileNameParameter="./lastStepBackgroundImageName"
                                                        fileReferenceParameter="./lastStepBackgroundImageReference"
                                                        mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
                                                        multiple="{Boolean}false"
                                                        name="./lastStepBackgroundImage"
                                                        title="Last Step Background Image"
                                                        uploadUrl="${suffix.path}"
                                                        useHTML5="{Boolean}true"/>
                                                </items>
                                            </lastStepBackgroundImage>
                                            <lastStepIcon
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="Last Step Icon Image"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <lastStepIcon
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                        autoStart="{Boolean}false"
                                                        class="cq-droptarget"
                                                        fileNameParameter="./lastStepIconImageName"
                                                        fileReferenceParameter="./lastStepIconImageReference"
                                                        mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
                                                        multiple="{Boolean}false"
                                                        name="./lastStepIcon"
                                                        title="Last Step Icon Image"
                                                        uploadUrl="${suffix.path}"
                                                        useHTML5="{Boolean}true"/>
                                                </items>
                                            </lastStepIcon>
                                            <lastStepDividerImage
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="Last Step Divider Image"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <lastStepDividerImage
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                        autoStart="{Boolean}false"
                                                        class="cq-droptarget"
                                                        fileNameParameter="./lastStepDividerImageName"
                                                        fileReferenceParameter="./lastStepDividerImageReference"
                                                        mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
                                                        multiple="{Boolean}false"
                                                        name="./lastStepDividerImage"
                                                        title="Last Step Divider Image"
                                                        uploadUrl="${suffix.path}"
                                                        useHTML5="{Boolean}true"/>
                                                </items>
                                            </lastStepDividerImage>
                                        </items>
                                    </column>
                                </items>
                            </lastStep>
                        </items>
                    </tabs>
                </items>
            </column>
        </items>
    </content>
</jcr:root>
