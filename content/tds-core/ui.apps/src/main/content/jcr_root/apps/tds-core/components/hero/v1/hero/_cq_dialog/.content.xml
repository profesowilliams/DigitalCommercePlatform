<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Hero"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[core.wcm.components.teaser.v1.editor,tds.authoring.components.editor.colorswatches,tds.authoring.components.hero]"
    helpPath="https://www.adobe.com/go/aem_cmp_teaser_v1"
    trackingFeature="core-components:teaser:v1">
    <content
        granite:class="cmp-teaser__editor cmp-dialog-swatches__editor"
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <actions jcr:primaryType="nt:unstructured">
                        <items jcr:primaryType="nt:unstructured">
                            <columns jcr:primaryType="nt:unstructured">
                                <items jcr:primaryType="nt:unstructured">
                                    <column jcr:primaryType="nt:unstructured">
                                        <items jcr:primaryType="nt:unstructured">
                                            <actions jcr:primaryType="nt:unstructured">
                                                <field
                                                    granite:class="cmp-teaser__editor-action cmp-hero__editor-action"
                                                    jcr:primaryType="nt:unstructured">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <id
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            emptyText="ID"
                                                            name="./id"/>
                                                    </items>
                                                </field>
                                            </actions>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </actions>
                    <textEnhancerConfiguration
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/include"
                        path="tds-core/components/authoring/dialog/text-enhancer-configuration/container"/>
                    <overlayColorOpacity
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Overlay Color and Opacity"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <color
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/colorfield"
                                        fieldDescription="Color"
                                        fieldLabel="Color"
                                        name="./color"
                                        showDefaultColors="{Boolean}false"
                                        showProperties="{Boolean}true"
                                        showSwatches="{Boolean}true"/>
                                </items>
                            </column>
                        </items>
                    </overlayColorOpacity>
                    <firstseethrougharea
                        jcr:primaryType="nt:unstructured"
                        jcr:title="First See Through"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <label
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="First see through section label"
                                        fieldLabel="Label"
                                        name="./firstSeeThroughSectionLabel"/>
                                    <iconUrl
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldDescription="Icon from DAM for displaying in the button"
                                        fieldLabel="Icon from DAM"
                                        name="./firstSeeThroughSectionIcon"
                                        rootPath="/content/dam"/>
                                    <heading
                                        granite:class="coral-Heading coral-Heading--4"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/heading"
                                        level="{Long}4"
                                        text="Link 1"/>
                                    <well
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/well">
                                        <items jcr:primaryType="nt:unstructured">
                                            <column
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <label
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldDescription="Enter label"
                                                        fieldLabel="Label"
                                                        name="./firstSeeThroughLink1Label"/>
                                                    <linkUrl
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                        fieldDescription="Enter link"
                                                        fieldLabel="Link"
                                                        name="./firstSeeThroughLink1Url"
                                                        rootPath="/content/techdata"/>
                                                    <linkTarget
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                        fieldDescription="Select this if the link has to open in a new tab"
                                                        name="./firstSeeThroughLink1Target"
                                                        text="Link to New tab"
                                                        uncheckedValue="false"
                                                        value="true"/>
                                                </items>
                                            </column>
                                        </items>
                                    </well>
                                    <heading2
                                        granite:class="coral-Heading coral-Heading--4"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/heading"
                                        level="{Long}4"
                                        text="Link 2"/>
                                    <well2
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/well">
                                        <items jcr:primaryType="nt:unstructured">
                                            <column
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <label
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldDescription="Enter label"
                                                        fieldLabel="Label"
                                                        name="./firstSeeThroughLink2Label"/>
                                                    <linkUrl
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                        fieldDescription="Enter link"
                                                        fieldLabel="Link"
                                                        name="./firstSeeThroughLink2Url"
                                                        rootPath="/content/techdata"/>
                                                    <linkTarget
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                        fieldDescription="Select this if the link has to open in a new tab"
                                                        name="./firstSeeThroughLink2Target"
                                                        text="Link to New tab"
                                                        uncheckedValue="false"
                                                        value="true"/>
                                                </items>
                                            </column>
                                        </items>
                                    </well2>
                                </items>
                            </column>
                        </items>
                    </firstseethrougharea>
                    <secondseethrougharea
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Second See Through"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <label
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Second see through section label"
                                        fieldLabel="Label"
                                        name="./secondSeeThroughSectionLabel"/>
                                    <iconUrl
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldDescription="Icon from DAM for displaying in the button"
                                        fieldLabel="Icon from DAM"
                                        name="./secondSeeThroughSectionIcon"
                                        rootPath="/content/dam"/>
                                    <heading
                                        granite:class="coral-Heading coral-Heading--4"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/heading"
                                        level="{Long}4"
                                        text="Link 1"/>
                                    <well
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/well">
                                        <items jcr:primaryType="nt:unstructured">
                                            <column
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <label
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldDescription="Enter label"
                                                        fieldLabel="Label"
                                                        name="./secondSeeThroughLink1Label"/>
                                                    <linkUrl
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                        fieldDescription="Enter link"
                                                        fieldLabel="Link"
                                                        name="./secondSeeThroughLink1Url"
                                                        rootPath="/content/techdata"/>
                                                    <linkTarget
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                        fieldDescription="Select this if the link has to open in a new tab"
                                                        name="./secondSeeThroughLink1Target"
                                                        text="Link to New tab"
                                                        uncheckedValue="false"
                                                        value="true"/>
                                                </items>
                                            </column>
                                        </items>
                                    </well>
                                    <heading2
                                        granite:class="coral-Heading coral-Heading--4"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/heading"
                                        level="{Long}4"
                                        text="Link 2"/>
                                    <well2
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/well">
                                        <items jcr:primaryType="nt:unstructured">
                                            <column
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <label
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldDescription="Enter label"
                                                        fieldLabel="Label"
                                                        name="./secondSeeThroughLink2Label"/>
                                                    <linkUrl
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                        fieldDescription="Enter link"
                                                        fieldLabel="Link"
                                                        name="./secondSeeThroughLink2Url"
                                                        rootPath="/content/techdata"/>
                                                    <linkTarget
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                        fieldDescription="Select this if the link has to open in a new tab"
                                                        name="./secondSeeThroughLink2Target"
                                                        text="Link to New tab"
                                                        uncheckedValue="false"
                                                        value="true"/>
                                                </items>
                                            </column>
                                        </items>
                                    </well2>
                                </items>
                            </column>
                        </items>
                    </secondseethrougharea>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
