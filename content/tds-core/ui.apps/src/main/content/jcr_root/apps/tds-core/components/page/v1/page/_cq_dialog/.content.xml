<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured">
    <content jcr:primaryType="nt:unstructured">
        <items jcr:primaryType="nt:unstructured">
            <tabs jcr:primaryType="nt:unstructured">
                <items jcr:primaryType="nt:unstructured">
                    <basic jcr:primaryType="nt:unstructured">
                        <items jcr:primaryType="nt:unstructured">
                            <column jcr:primaryType="nt:unstructured">
                                <items jcr:primaryType="nt:unstructured">
                                    <navigationTitle
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Country name for Header Navigation."
                                            fieldLabel="Header Navigation Title"
                                            name="./navigationTitle"/>
                                    <pageMenuIcon
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="The Font Awesome icon that will be displayed in the Mega Menu for this page"
                                            fieldLabel="Menu Icon to be displayed on Mega Menu"
                                            value="fas fa-server"
                                            name="./pageMenuIcon"/>
                                    <isPrivatePage
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                            fieldDescription="Toggle for whether this is a Private Page or Public Page"
                                            text="Private Page?"
                                            value="true"
                                            name="./isPrivatePage"/>
                                    <contentFragmentPath
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                            fieldLabel="Content fragment path"
                                            name="./cfPath"
                                            rootPath="/content/dam/tdscore">
                                    </contentFragmentPath>
                                    <tagCategories
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="cq/gui/components/coral/common/form/tagfield"
                                            fieldLabel="Tags for Categorization"
                                            name="./tagCategories"
                                            multiple="{Boolean}true"
                                            rootPath="/content/cq:tags">
                                    </tagCategories>
                                    <errorPages
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                            fieldDescription="Error pages for this content tree"
                                            fieldLabel="Error Pages Path"
                                            name="./errorPages"
                                            rootPath="/content/tdscore">
                                    </errorPages>
                                    <errorCode
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Define error code for the page."
                                            fieldLabel="Error Code"
                                            name="./errorCode"/>

                                    <errorName
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Define error Name for the page."
                                            fieldLabel="Error Name"
                                            name="./errorName"/>
                                    <embedScript
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                            fieldDescription="Build JS/CSS script based on the custom paths from DAM or elsewhere."
                                            fieldLabel="Embed JS/CSS script"
                                            rows="10"
                                            name="./embedJSCSSScript"/>
                                    <currency
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Define currency at the locale page, else default to USD."
                                            fieldLabel="Currency Code"
                                            name="./currency"/>
                                    <pageType
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Defines pagetype for the page."
                                            fieldLabel="Page Type"
                                            name="./pageType"/>
                                </items>
                            </column>
                        </items>
                    </basic>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
