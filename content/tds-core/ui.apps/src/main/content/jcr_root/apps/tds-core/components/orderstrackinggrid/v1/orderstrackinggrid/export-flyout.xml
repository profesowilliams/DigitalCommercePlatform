<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    sling:resourceType="granite/ui/components/coral/foundation/container">
    <items jcr:primaryType="nt:unstructured">
        <exportFlyoutGroup
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/well">
            <items jcr:primaryType="nt:unstructured">
                <exportFlyoutTitle
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                    fieldLabel="Export Flyout Title"
                    name="./exportFlyoutTitle"/>
                <exportFlyoutDescriptionMessage
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                    fieldLabel="InTouch can export up to 500 orders at a time.  Apply filters to refine your results."
                    name="./exportFlyoutDescriptionMessage"/>
                <exportFlyoutDescription
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                    fieldLabel="Export Flyout Description"
                    name="./exportFlyoutDescription"/>
                <exportFlyoutSecondaryDescription
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                    fieldLabel="Export Flyout Secondary Description"
                    name="./exportFlyoutSecondaryDescription"/>
                <exportFlyoutButton
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                    fieldLabel="Export Flyout Button Label"
                    name="./exportFlyoutButton"/>
                <exportSuccessMessage
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                    fieldLabel="Export Success Message Label"
                    name="./exportSuccessMessage"/>
                <exportFailedMessage
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                    fieldLabel="Export Failed Message Label"
                    name="./exportFailedMessage"/>
                <exportOptionsList
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                    composite="{Boolean}true"
                    fieldDescription="Click 'Add' to add labels"
                    fieldLabel="Section 1 Description">
                    <field
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        name="./exportOptionsList">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <label
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter label"
                                        fieldLabel="Label"
                                        name="./label"
                                        required="{Boolean}true" />
                                    <key
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter key"
                                        fieldLabel="Key"
                                        name="./key"
                                        required="{Boolean}true" />
                                </items>
                            </column>
                        </items>
                    </field>
                </exportOptionsList>
                <exportSecondaryOptionsList
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                    composite="{Boolean}true"
                    fieldDescription="Click 'Add' to add labels"
                    fieldLabel="Section 2 Description">
                    <field
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        name="./exportSecondaryOptionsList">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <label
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter label"
                                        fieldLabel="Label"
                                        name="./label"
                                        required="{Boolean}true" />
                                    <key
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter key"
                                        fieldLabel="Key"
                                        name="./key"
                                        required="{Boolean}true" />
                                </items>
                            </column>
                        </items>
                    </field>
                </exportSecondaryOptionsList>
            </items>
        </exportFlyoutGroup>
    </items>
</jcr:root>
