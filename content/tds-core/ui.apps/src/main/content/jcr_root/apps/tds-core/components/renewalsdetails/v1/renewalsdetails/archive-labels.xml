<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    sling:resourceType="granite/ui/components/coral/foundation/container">
    <items jcr:primaryType="nt:unstructured">
        <archiveLablelsGroup
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/well">
            <items jcr:primaryType="nt:unstructured">
                <archive
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                    fieldDescription="mainDashboard.renewal.archive.label.archive"
                    fieldLabel="Archive"
                    name="./archiveLabels/archive"/>
                <archiveToasterSuccess
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="mainDashboard.renewal.archive.label.archiveToasterSuccess"
                        fieldLabel="Archive toaster success"
                        name="./archiveLabels/archiveToasterSuccess"/>
                <archiveToasterFail
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="mainDashboard.renewal.archive.label.archiveToasterFail"
                        fieldLabel="Archive toaster Fail"
                        name="./archiveLabels/archiveToasterFail"/>
                <restoreToasterSuccess
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="mainDashboard.renewal.archive.label.restoreToasterSuccess"
                        fieldLabel="Restore toaster success"
                        name="./archiveLabels/restoreToasterSuccess"/>
                <restoreToasterFail
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="mainDashboard.renewal.archive.label.restoreToasterFail"
                        fieldLabel="Restore toaster Fail"
                        name="./archiveLabels/restoreToasterFail"/>
                <undoArchive
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="mainDashboard.renewal.archive.label.undo"
                        fieldLabel="Undo archive"
                        name="./archiveLabels/undo"/>

            </items>
        </archiveLablelsGroup>
    </items>
</jcr:root>