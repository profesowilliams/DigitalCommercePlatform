# redirect root
RedirectMatch "^/$" "/us"

# apac
RewriteRule ^/sg$ /content/${CONTENT_FOLDER_NAME}/apac/sg/en.html [PT,L]
RewriteRule ^/sg/en$ /content/${CONTENT_FOLDER_NAME}/apac/sg/en.html [PT,L]
RewriteRule ^/au$ /content/${CONTENT_FOLDER_NAME}/apac/au/en.html [PT,L]
RewriteRule ^/au/en$ /content/${CONTENT_FOLDER_NAME}/apac/au/en.html [PT,L]
RewriteRule ^/hk$ /content/${CONTENT_FOLDER_NAME}/apac/hk/en.html [PT,L]
RewriteRule ^/hk/en$ /content/${CONTENT_FOLDER_NAME}/apac/hk/en.html [PT,L]
RewriteRule ^/hk/tdcs$ /content/${CONTENT_FOLDER_NAME}/apac/hk/tdcs.html [PT,L]
RewriteRule ^/in$ /content/${CONTENT_FOLDER_NAME}/apac/in/en.html [PT,L]
RewriteRule ^/in/en$ /content/${CONTENT_FOLDER_NAME}/apac/in/en.html [PT,L]
RewriteRule ^/my$ /content/${CONTENT_FOLDER_NAME}/apac/my/en.html [PT,L]
RewriteRule ^/my/en$ /content/${CONTENT_FOLDER_NAME}/apac/my/en.html [PT,L]
RewriteRule ^/nz$ /content/${CONTENT_FOLDER_NAME}/apac/nz/en.html [PT,L]
RewriteRule ^/nz/en$ /content/${CONTENT_FOLDER_NAME}/apac/nz/en.html [PT,L]
RewriteRule ^/apac$ /content/${CONTENT_FOLDER_NAME}/apac/apac/en.html [PT,L]

# americas
RewriteRule ^/us$ /content/${CONTENT_FOLDER_NAME}/americas/us/en.html [PT,L]
RewriteRule ^/us/en$ /content/${CONTENT_FOLDER_NAME}/americas/us/en.html [PT,L]

RewriteCond %{REQUEST_URI} !^/apps
RewriteCond %{REQUEST_URI} !^/bin
RewriteCond %{REQUEST_URI} !^/content
RewriteCond %{REQUEST_URI} !^/etc
RewriteCond %{REQUEST_URI} !^/home
RewriteCond %{REQUEST_URI} !^/libs
RewriteCond %{REQUEST_URI} !^/saml_login
RewriteCond %{REQUEST_URI} !^/system
RewriteCond %{REQUEST_URI} !^/tmp
RewriteCond %{REQUEST_URI} !^/var
RewriteCond %{REQUEST_URI} (.html|.jpe?g|.png|.svg)$
RewriteRule ^/us(.*)$ /content/${CONTENT_FOLDER_NAME}/americas/us$1 [PT,L]

RewriteCond %{REQUEST_URI} !^/apps
RewriteCond %{REQUEST_URI} !^/bin
RewriteCond %{REQUEST_URI} !^/content
RewriteCond %{REQUEST_URI} !^/etc
RewriteCond %{REQUEST_URI} !^/home
RewriteCond %{REQUEST_URI} !^/libs
RewriteCond %{REQUEST_URI} !^/saml_login
RewriteCond %{REQUEST_URI} !^/system
RewriteCond %{REQUEST_URI} !^/tmp
RewriteCond %{REQUEST_URI} !^/var
RewriteCond %{REQUEST_URI} (.html|.jpe?g|.png|.svg)$
RewriteRule ^/(.*)$ /content/${CONTENT_FOLDER_NAME}/apac/$1 [PT,L]