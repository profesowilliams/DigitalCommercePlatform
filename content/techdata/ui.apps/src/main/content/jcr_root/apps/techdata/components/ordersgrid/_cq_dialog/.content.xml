<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Orders Grid Properties"
          sling:resourceType="cq/gui/components/authoring/dialog">
    <content
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <column
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <tabs
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/tabs"
                            maximized="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <properties
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Properties"
                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                    margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <label
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Label"
                                                    name="./label"/>
                                            <itemsPerPage
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Items Per Page"
                                                    name="./itemsPerPage"/>
                                            <orderDetailUrl
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                    fieldLabel="Link"
                                                    fieldDescription="Enter link"
                                                    jcr:primaryType="nt:unstructured"
                                                    rootPath="/content/techdata"
                                                    name="./orderDetailUrl" />
                                            <paginationStyle
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/radiogroup"
                                                    fieldLabel="Select Pagination Style"
                                                    name="./paginationStyle">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <pages
                                                            jcr:primaryType="nt:unstructured"
                                                            checked="{Boolean}true"
                                                            text="Pages"
                                                            value="pages"/>
                                                    <scroll
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Scroll"
                                                            value="scroll"/>
                                                    <none
                                                            jcr:primaryType="nt:unstructured"
                                                            text="None"
                                                            value="none"/>
                                                </items>
                                            </paginationStyle>
                                            <defaultSortingColumnKey
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="/libs/granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Default Sorting Column Key"
                                                    fieldLabel="Default Sorting Column Key"
                                                    name="./defaultSortingColumnKey"/>
                                            <defaultSortingDirection
                                                    jcr:primaryType="nt:unstructured"
                                                    jcr:title="Sorting Direction"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/radiogroup"
                                                    name="./defaultSortingDirection"
                                                    margin="{Boolean}true">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <asc
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Ascending Order"
                                                            value="asc"/>
                                                    <desc
                                                            jcr:primaryType="nt:unstructured"
                                                            checked="{Boolean}true"
                                                            text="Descending Order"
                                                            value="desc"/>
                                                </items>
                                            </defaultSortingDirection>
											<menuCopy
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Copy Label"
                                                    name="./menuCopy"/>
											<menuCopyWithHeaders
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Copy with headers Label"
                                                    name="./menuCopyWithHeaders"/>
											<menuExport
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Export Label"
                                                    name="./menuExport"/>
											<menuCsvExport
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="CSV Export Label"
                                                    name="./menuCsvExport"/>
											<menuExcelExport
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Excel Export Label"
                                                    name="./menuExcelExport"/>
											<menuOpenLink
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Open link Label"
                                                    name="./menuOpenLink"/>
											<menuCopyLink
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Copy link Label"
                                                    name="./menuCopyLink"/>
                                        </items>
                                    </column>
                                </items>
                            </properties>
                            <elements
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Items"
                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                    margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <actions
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                    composite="{Boolean}true"
                                                    fieldDescription="Click 'Add' to add labels">
                                                <field
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                                        name="./columnList">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <column
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <columnLabel
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldDescription="Enter column label"
                                                                        fieldLabel="Column Label"
                                                                        name="./columnLabel"/>
                                                                <columnKey
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldDescription="Enter Column key for the column"
                                                                        fieldLabel="Column Key"
                                                                        name="./columnKey"/>
                                                                <sortable
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                                        name="./sortable"
                                                                        text="Sortable"
                                                                        uncheckedValue="false"
                                                                        value="{Boolean}true"/>
                                                                <sortable-typehint
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                                                        name="./sortable@TypeHint"
                                                                        value="Boolean"/>
                                                            </items>
                                                        </column>
                                                    </items>
                                                </field>
                                            </actions>
                                        </items>
                                    </column>
                                </items>
                            </elements>
                            <tab3
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Icons Tab"
                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                    margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <actions
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                    composite="{Boolean}true"
                                                    fieldDescription="Click 'Add' to add key and value for icons">
                                                <field
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                                        name="./iconList">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <column
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <iconKey
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldDescription="Enter Icon Key"
                                                                        fieldLabel="Icon Key"
                                                                        name="./iconKey"/>
                                                                <iconValue
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldDescription="Enter Icon Value"
                                                                        fieldLabel="Icon Value"
                                                                        name="./iconValue"/>
                                                                <iconText
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldDescription="Enter Icon Text"
                                                                        fieldLabel="Icon Text"
                                                                        name="./iconText"/>
                                                            </items>
                                                        </column>
                                                    </items>
                                                </field>
                                            </actions>
                                        </items>
                                    </column>
                                </items>
                            </tab3>
                            <invoiceModal
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Invoice Modal"
                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <title
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Enter Title"
                                            fieldLabel="Modal Title"
                                            name="./title"/>
                                    <buttonLabel
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Enter Button Label"
                                            fieldLabel="Button Label"
                                            name="./buttonLabel"/>
                                    <buttonIcon
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Enter Button Icon"
                                            fieldLabel="Button Icon"
                                            name="./buttonIcon"/>
                                    <content
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                            fieldDescription="Enter Content"
                                            fieldLabel="Content"
                                            name="./content"/>
                                    <pendingInfo
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Enter Pending Info"
                                            fieldLabel="Pending Info"
                                            name="./pendingInfo"/>
                                    <pendingInfo
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Download Invoices Error Message"
                                            name="./errorMessage"/>
                                </items>
                            </invoiceModal>
                            <labelList
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Label Tab"
                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                    margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <actions
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                    composite="{Boolean}true"
                                                    fieldDescription="Click 'Add' to add label key and label value">
                                                <field
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                                        name="./labelList">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <column
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <labelKey
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldDescription="Label Key"
                                                                        fieldLabel="Label Key"
                                                                        name="./labelKey"/>
                                                                <labelValue
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldDescription="Enter Icon Value"
                                                                        fieldLabel="label Value"
                                                                        name="./labelValue"/>
                                                            </items>
                                                        </column>
                                                    </items>
                                                </field>
                                            </actions>
                                        </items>
                                    </column>
                                </items>
                            </labelList>
                            <trackingInfo
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Tracking Info Configuration"
                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                    margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <carrierInfo
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                    composite="{Boolean}true"
                                                    fieldLabel="Click 'Add' to add Carrier key and logo paths"
                                                    fieldDescription="Click 'Add' to add Carrier key and logo paths">
                                                <field
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                                        name="./trackingIcons">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <column
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <carrierKey
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldDescription="Carrier Logo Key"
                                                                        fieldLabel="Carrier Key"
                                                                        name="./carrier"/>
                                                                <logoPath
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                        fieldDescription="Carrier Logo Path"
                                                                        fieldLabel="Carrier Logo Path"
                                                                        name="./logoPath"/>
                                                            </items>
                                                        </column>
                                                    </items>
                                                </field>
                                            </carrierInfo>
                                            <modalTitle
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Tracking Info Modal Title"
                                                    fieldLabel="Tracking Info Modal Title"
                                                    name="./modalTitle"/>
                                            <multipleOrderInformation
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Description for Multiple Orders"
                                                    fieldLabel="Description for Multiple Orders"
                                                    name="./multipleOrderInformation"/>
                                        </items>
                                    </column>
                                </items>
                            </trackingInfo>
                            <searchCriteria
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Search Tab"
                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                    margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <searchTitle
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Enter Title"
                                                    fieldLabel="Search Title"
                                                    name="./searchTitle"/>
                                            <searchButtonLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Enter Label for Search Button"
                                                    fieldLabel="Search Button Label"
                                                    name="./searchButtonLabel"/>
                                            <clearButtonLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Enter Clear Button Label"
                                                    fieldLabel="Clear Button Label"
                                                    name="./clearButtonLabel"/>
                                            <inputPlaceholder
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Enter Input"
                                                    fieldLabel="Input Placeholder"
                                                    name="./inputPlaceholder"/>
                                            <labelButtonFilter
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Open Orders Button Label"
                                                    fieldLabel="Open Orders Button Label"
                                                    name="./labelButtonFilter"/>
                                            <keywordDropdownLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Label"
                                                    name="./keywordDropdownLabel"/>
                                            <actions
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                    composite="{Boolean}true"
                                                    fieldDescription="Click 'Add' to add key and value for drop down">
                                                <field
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                                        name="./keywordList">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <column
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <key
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldDescription="Enter Key"
                                                                        fieldLabel="Key"
                                                                        name="./key"/>
                                                                <labelValue
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldDescription="Enter Value"
                                                                        fieldLabel="Value"
                                                                        name="./value"/>
                                                            </items>
                                                        </column>
                                                    </items>
                                                </field>
                                            </actions>
                                            <vendorDropdownLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Vendor Label"
                                                    name="./vendorDropdownLabel"/>
                                            <vendorActions
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                    composite="{Boolean}true"
                                                    fieldDescription="Click 'Add' to add key and value for drop down">
                                                <field
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                                        name="./vendorList">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <column
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <key
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldDescription="Enter Key"
                                                                        fieldLabel="Vendor Key"
                                                                        name="./vendorKey"/>
                                                                <labelValue
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldDescription="Enter Value"
                                                                        fieldLabel="Vednor Value"
                                                                        name="./vendorValue"/>
                                                            </items>
                                                        </column>
                                                    </items>
                                                </field>
                                            </vendorActions>
                                            <methodDropdownLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="method Label"
                                                    name="./methodDropdownLabel"/>
                                            <methodActions
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                    composite="{Boolean}true"
                                                    fieldDescription="Click 'Add' to add key and value for method drop down">
                                                <field
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                                        name="./methodList">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <column
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <key
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldDescription="Enter Key"
                                                                        fieldLabel="Method Key"
                                                                        name="./methodKey"/>
                                                                <labelValue
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldDescription="Enter Value"
                                                                        fieldLabel="Method Value"
                                                                        name="./methodValue"/>
                                                            </items>
                                                        </column>
                                                    </items>
                                                </field>
                                            </methodActions>
                                            <fromLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Enter From Label"
                                                    fieldLabel="From Label"
                                                    name="./fromLabel"/>
                                            <toLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Enter To Label"
                                                    fieldLabel="To Label"
                                                    name="./toLabel"/>
                                            <datePlaceholder
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Enter Date Placeholder"
                                                    fieldLabel="Date Placeholder"
                                                    name="./datePlaceholder"/>
                                        </items>
                                    </column>
                                </items>
                            </searchCriteria>
                        </items>
                    </tabs>
                </items>
            </column>
        </items>
    </content>
</jcr:root>

