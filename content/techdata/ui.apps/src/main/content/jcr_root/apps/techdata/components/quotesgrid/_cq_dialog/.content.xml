<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Properties"
          sling:resourceType="cq/gui/components/authoring/dialog">
    <content
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <column
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <tabs
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/tabs"
                            maximized="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <properties
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Properties"
                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                    margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <label
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Label"
                                                    name="./label"/>
                                            <itemsPerPage
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Items Per Page"
                                                    name="./itemsPerPage"/>
                                            <quoteDetailUrl
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                    fieldLabel="Link"
                                                    fieldDescription="Enter link"
                                                    jcr:primaryType="nt:unstructured"
                                                    rootPath="/content/techdata"
                                                    name="./quoteDetailUrl" />
                                            <multipleSPADealLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="SPA Deals Label for Multiple items"
                                                    name="./spaDealsIdLabel"/>
                                            <paginationStyle
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/radiogroup"
                                                    fieldLabel="Select Pagination Style"
                                                    name="./paginationStyle">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <pages
                                                            jcr:primaryType="nt:unstructured"
                                                            checked="{Boolean}true"
                                                            text="Pages"
                                                            value="pages"/>
                                                    <scroll
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Scroll"
                                                            value="scroll"/>
                                                    <none
                                                            jcr:primaryType="nt:unstructured"
                                                            text="None"
                                                            value="none"/>
                                                </items>
                                            </paginationStyle>
                                            <defaultSortingColumnKey
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="/libs/granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Default Sorting Column Key"
                                                    fieldLabel="Default Sorting Column Key"
                                                    name="./defaultSortingColumnKey"/>
                                            <defaultSortingDirection
                                                    jcr:primaryType="nt:unstructured"
                                                    jcr:title="Sorting Direction"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/radiogroup"
                                                    name="./defaultSortingDirection"
                                                    margin="{Boolean}true">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <asc
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Ascending Order"
                                                            value="asc"/>
                                                    <desc
                                                            jcr:primaryType="nt:unstructured"
                                                            checked="{Boolean}true"
                                                            text="Descending Order"
                                                            value="desc"/>
                                                </items>
                                            </defaultSortingDirection>
											<menuCopy
													jcr:primaryType="nt:unstructured"
													sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
													fieldLabel="Context Menu Copy Label"
													name="./menuCopy"/>
											<menuCopyWithHeaders
													jcr:primaryType="nt:unstructured"
													sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
													fieldLabel="Context Menu Copy with headers Label"
													name="./menuCopyWithHeaders"/>
											<menuExport
													jcr:primaryType="nt:unstructured"
													sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
													fieldLabel="Context Menu Export Label"
													name="./menuExport"/>
											<menuCsvExport
													jcr:primaryType="nt:unstructured"
													sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
													fieldLabel="Context Menu CSV Export Label"
													name="./menuCsvExport"/>
											<menuExcelExport
													jcr:primaryType="nt:unstructured"
													sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
													fieldLabel="Context Menu Excel Export Label"
													name="./menuExcelExport"/>
											<menuOpenLink
													jcr:primaryType="nt:unstructured"
													sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
													fieldLabel="Context Menu Open link Label"
													name="./menuOpenLink"/>
											<menuCopyLink
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Context Menu Copy link Label"
                                                    name="./menuCopyLink"/>
                                            <gridErrorMessage
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Quotes Grid Error Message"
                                                    description="Error message that will be displayed when an error occurs on creating a quote."
                                                    name="./gridErrorMessage"/>
										</items>
                                    </column>
                                </items>
                            </properties>
                            <elements
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Items"
                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                    margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <actions
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                    composite="{Boolean}true"
                                                    fieldDescription="Click 'Add' to add labels">
                                                <field
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                                        name="./columnList">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <column
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <columnLabel
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldDescription="Enter column label"
                                                                        fieldLabel="Column Label"
                                                                        name="./columnLabel"/>
                                                                <columnKey
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldDescription="Enter Column key for the column"
                                                                        fieldLabel="Column Key"
                                                                        name="./columnKey"/>
                                                                <sortable
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                                        name="./sortable"
                                                                        text="Sortable"
                                                                        uncheckedValue="false"
                                                                        value="{Boolean}true"/>
                                                                <sortable-typehint
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                                                        name="./sortable@TypeHint"
                                                                        value="Boolean"/>
                                                            </items>
                                                        </column>
                                                    </items>
                                                </field>
                                            </actions>
                                        </items>
                                    </column>
                                </items>
                            </elements>
                            <tab3
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Icons Tab"
                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                    margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <actions
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                    composite="{Boolean}true"
                                                    fieldDescription="Click 'Add' to add key and value for icons">
                                                <field
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                                        name="./iconList">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <column
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <iconKey
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldDescription="Enter Icon Key"
                                                                        fieldLabel="Icon Key"
                                                                        name="./iconKey"/>
                                                                <iconValue
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldDescription="Enter Icon Value"
                                                                        fieldLabel="Icon Value"
                                                                        name="./iconValue"/>
                                                                <iconText
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldDescription="Enter Icon Text"
                                                                        fieldLabel="Icon Text"
                                                                        name="./iconText"/>
                                                            </items>
                                                        </column>
                                                    </items>
                                                </field>
                                            </actions>
                                        </items>
                                    </column>
                                </items>
                            </tab3>
                            <searchCriteria
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Search Tab"
                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                    margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <searchTitle
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Enter Title"
                                                    fieldLabel="Search Title"
                                                    name="./searchTitle"/>
                                            <searchButtonLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Enter Label for Search Button"
                                                    fieldLabel="Search Button Label"
                                                    name="./searchButtonLabel"/>
                                            <clearButtonLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Enter Clear Button Label"
                                                    fieldLabel="Clear Button Label"
                                                    name="./clearButtonLabel"/>
                                            <inputPlaceholder
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Enter Input"
                                                    fieldLabel="Input Placeholder"
                                                    name="./inputPlaceholder"/>
                                            <keywordDropdownLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Label"
                                                    name="./keywordDropdownLabel"/>
                                            <actions
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                    composite="{Boolean}true"
                                                    fieldDescription="Click 'Add' to add key and value for drop down">
                                                <field
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                                        name="./keywordList">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <column
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <key
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldDescription="Enter Key"
                                                                        fieldLabel="Key"
                                                                        name="./key"/>
                                                                <labelValue
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldDescription="Enter Value"
                                                                        fieldLabel="Value"
                                                                        name="./value"/>
                                                            </items>
                                                        </column>
                                                    </items>
                                                </field>
                                            </actions>
                                            <vendorDropdownLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Vendor Label"
                                                    name="./vendorDropdownLabel"/>
                                            <vendorActions
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                    composite="{Boolean}true"
                                                    fieldDescription="Click 'Add' to add key and value for drop down">
                                                <field
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                                        name="./vendorList">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <column
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <key
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldDescription="Enter Key"
                                                                        fieldLabel="Vendor Key"
                                                                        name="./vendorKey"/>
                                                                <labelValue
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldDescription="Enter Value"
                                                                        fieldLabel="Vednor Value"
                                                                        name="./vendorValue"/>
                                                            </items>
                                                        </column>
                                                    </items>
                                                </field>
                                            </vendorActions>
                                            <fromLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Enter From Label"
                                                    fieldLabel="From Label"
                                                    name="./fromLabel"/>
                                            <toLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Enter To Label"
                                                    fieldLabel="To Label"
                                                    name="./toLabel"/>
                                            <datePlaceholder
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Enter Date Placeholder"
                                                    fieldLabel="Date Placeholder"
                                                    name="./datePlaceholder"/>
                                        </items>
                                    </column>
                                </items>
                            </searchCriteria>
                        </items>
                    </tabs>
                </items>
            </column>
        </items>
    </content>
</jcr:root>

