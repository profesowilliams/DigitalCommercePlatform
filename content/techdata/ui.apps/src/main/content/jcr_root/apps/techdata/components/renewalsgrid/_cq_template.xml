<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    configDetailUrlSuffix="?id={id}"
    quoteDetailUrlSuffix="?id={id}"
    modalTitle="Config ID"
    modalContent="There are multiple items associated with this Config ID. Please choose the item you would like to review:"
    modalQuotePreffixLabel="Quote: "
    paginationStyle="scroll"
    itemsPerPage="25"
    actionColumnLabel="Create Quote"
    createQuoteUrlSuffix="?id={id}&amp;isEstimateId={is-estimated-id}&amp;vendor={vendor}"
    defaultSortingColumnKey="created"
    defaultSortingDirection="desc"
    showResultLabel="Show Result">
>
    <configurationTypesList jcr:primaryType="nt:unstructured">
        <item0
            jcr:primaryType="nt:unstructured"
            configurationTypesValue="End User"
            configurationTypesKey="EndUser"
        />
        <item1
            jcr:primaryType="nt:unstructured"
            configurationTypesValue="Config ID"
            configurationTypesKey="Id"
        />
        <item2
            jcr:primaryType="nt:unstructured"
            configurationTypesValue="Config Name"
            configurationTypesKey="ConfigName"
        />
    </configurationTypesList>
    <columnList jcr:primaryType="nt:unstructured">
        <item0
            jcr:primaryType="nt:unstructured"
            columnLabel="Reseller name"
            columnKey="reseller"
            sortable="{Boolean}false"
            columnType="plainText"
        />
        <item1
            jcr:primaryType="nt:unstructured"
            columnLabel="End user name"
            columnKey="endUser"
            sortable="{Boolean}false"
            columnType="plainText"
        />
        <item2
            jcr:primaryType="nt:unstructured"
            columnLabel="Vendor and program"
            columnKey="vendor"
            sortable="{Boolean}false"
            columnType="plainText"
        />
        <item3
            jcr:primaryType="nt:unstructured"
            columnLabel="Agreement No"
            columnKey="agreementNumber"
            sortable="{Boolean}false"
            columnType="plainText"
        />
        <item4
            jcr:primaryType="nt:unstructured"
            columnLabel="Renewal Plan"
            columnKey="renewal"
            sortable="{Boolean}false"
            columnType="plainText"
        />
        <item5
            jcr:primaryType="nt:unstructured"
            columnLabel="Due in (days)"
            columnKey="dueDateDays"
            sortable="{Boolean}true"
            columnType="plainText"
        />
        <item6
            jcr:primaryType="nt:unstructured"
            columnLabel="Due Date"
            columnKey="dueDate"
            sortable="{Boolean}false"
            columnType="plainText"
        />
        <item7
            jcr:primaryType="nt:unstructured"
            columnLabel="Price (USD)"
            columnKey="price"
            sortable="{Boolean}false"
            columnType="plainText"
        />
        <item8
            jcr:primaryType="nt:unstructured"
            columnLabel="Actions"
            columnKey="actions"
            sortable="{Boolean}false"
            columnType="buttonList"
        />
    </columnList>
    <statusLabelsList jcr:primaryType="nt:unstructured">
        <item0
            jcr:primaryType="nt:unstructured"
            labelKey="Multiple"
            labelValue="Multiple"
        />
        <item1
            jcr:primaryType="nt:unstructured"
            labelKey="Pending"
            labelValue="Pending"
        />
    </statusLabelsList>
    <configurationTypesList jcr:primaryType="nt:unstructured">
        <item0
            jcr:primaryType="nt:unstructured"
            configurationTypesKey="Estimate"
            configurationTypesValue="Estimate"
        />
        <item1
            jcr:primaryType="nt:unstructured"
            configurationTypesKey="VendorQuote"
            configurationTypesValue="Vendor Quote"
        />
        <item2
            jcr:primaryType="nt:unstructured"
            configurationTypesKey="Deal"
            configurationTypesValue="Deal"
        />
    </configurationTypesList>
    <keywordList jcr:primaryType="nt:unstructured">
        <item0
            jcr:primaryType="nt:unstructured"
            key="EndUser"
            value="End User"
        />
        <item1
            jcr:primaryType="nt:unstructured"
            key="Id"
            value="Config ID"
        />
        <item2
            jcr:primaryType="nt:unstructured"
            key="ConfigName"
            value="Config Name"
        />
    </keywordList>
    <filterList   
        jcr:primaryType="nt:unstructured">
        <item0          
            jcr:primaryType="nt:unstructured"
            accordionLabel="Vendors and Program"
            filterField="ProgramName">
            <filtersOptionsList            
                jcr:primaryType="nt:unstructured">
                <item0            
                    jcr:primaryType="nt:unstructured"
                    filterOptionLabel="Veeam">
                    <subFilterOptionList                       
                        jcr:primaryType="nt:unstructured">
                        <item0                            
                            jcr:primaryType="nt:unstructured"
                            subFilterOptionsLabel="Pro Partner"/>
                        <item1               
                            jcr:primaryType="nt:unstructured"
                            subFilterOptionsLabel="Pro Partner Subscriber"/>
                    </subFilterOptionList>
                </item0>
                <item1                 
                    jcr:primaryType="nt:unstructured"
                    filterOptionLabel="VMWare">
                    <subFilterOptionList               
                        jcr:primaryType="nt:unstructured">
                        <item0             
                            jcr:primaryType="nt:unstructured"
                            subFilterOptionsLabel="Support and Subscriber"/>
                    </subFilterOptionList>
                </item1>
            </filtersOptionsList>
        </item0>
        <item1      
            jcr:primaryType="nt:unstructured"
            accordionLabel="End User Type"
            filterField="endUserType">
            <filtersOptionsList             
                jcr:primaryType="nt:unstructured">
                <item0            
                    jcr:primaryType="nt:unstructured"
                    filterOptionLabel="Academic"/>
                <item1                  
                    jcr:primaryType="nt:unstructured"
                    filterOptionLabel="Educational"/>
            </filtersOptionsList>
        </item1>
        <item2      
            jcr:primaryType="nt:unstructured"
            accordionLabel="Date"
            filterField="date"/>
    </filterList>
    <searchOptionsList jcr:primaryType="nt:unstructured">
        <item0
            jcr:primaryType="nt:unstructured"
            searchKey="serial"
            searchLabel="Serial №"/>
        <item1
            jcr:primaryType="nt:unstructured"
            searchKey="agreement"
            searchLabel="Agreement №"/>
        <item2
            jcr:primaryType="nt:unstructured"
            searchKey="instance"
            searchLabel="Instance №"/>
        <item3
            jcr:primaryType="nt:unstructured"
            searchKey="resellerPreviousOrder"
            searchLabel="Reseller Previous Order №"/>
        <item4
            jcr:primaryType="nt:unstructured"
            searchKey="endUserPreviousOrder"
            searchLabel="End User Previous Order №"/>
    </searchOptionsList>
</jcr:root>
