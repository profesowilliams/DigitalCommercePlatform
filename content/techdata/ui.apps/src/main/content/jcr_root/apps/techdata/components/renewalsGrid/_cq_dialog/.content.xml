<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Properties"
          sling:resourceType="cq/gui/components/authoring/dialog">
    <content
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <column
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <tabs
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/tabs"
                            maximized="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <properties
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Properties"
                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                    margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <itemsPerPage
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Items Per Page"
                                                    required="{Boolean}true"
                                                    name="./itemsPerPage"/>
                                            <multipleSPADealLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="SPA Deals Label for Multiple items"
                                                    name="./spaDealsIdLabel"/>
                                            <paginationStyle
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/radiogroup"
                                                    fieldLabel="Select Pagination Style"
                                                    name="./paginationStyle">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <pages
                                                            jcr:primaryType="nt:unstructured"
                                                            checked="{Boolean}true"
                                                            text="Pages"
                                                            value="pages"/>
                                                    <scroll
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Scroll"
                                                            value="scroll"/>
                                                    <none
                                                            jcr:primaryType="nt:unstructured"
                                                            text="None"
                                                            value="none"/>
                                                </items>
                                            </paginationStyle>
                                            <defaultSortingColumnKey
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="/libs/granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Default Sorting Column Key"
                                                    fieldLabel="Default Sorting Column Key"
                                                    name="./defaultSortingColumnKey"/>
                                            <defaultSortingDirection
                                                    jcr:primaryType="nt:unstructured"
                                                    jcr:title="Sorting Direction"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/radiogroup"
                                                    name="./defaultSortingDirection"
                                                    margin="{Boolean}true">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <asc
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Ascending Order"
                                                            value="asc"/>
                                                    <desc
                                                            jcr:primaryType="nt:unstructured"
                                                            checked="{Boolean}true"
                                                            text="Descending Order"
                                                            value="desc"/>
                                                </items>
                                            </defaultSortingDirection>
                                        </items>
                                    </column>
                                </items>
                            </properties>
                            <elements
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Items"
                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                    margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <actions
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                composite="{Boolean}true"
                                                fieldLabel="Grid Columns Definition"
                                                fieldDescription="Click 'Add' to add labels">
                                                <field
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                                        name="./columnList">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <column
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <columnLabel
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        required="{Boolean}true"
                                                                        fieldDescription="Enter column label"
                                                                        fieldLabel="Column Label"
                                                                        name="./columnLabel"/>
                                                                <columnKey
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        required="{Boolean}true"
                                                                        fieldDescription="Enter Column key for the column"
                                                                        fieldLabel="Column Key"
                                                                        name="./columnKey"/>
                                                                <columnType
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                                        fieldLabel="Column Type"
                                                                        name="./columnType">
                                                                        <items
                                                                                jcr:primaryType="nt:unstructured">
                                                                                <plainText
                                                                                        jcr:primaryType="nt:unstructured"
                                                                                        text="Plain Text"
                                                                                        value="plainText"/>
                                                                                <date
                                                                                        jcr:primaryType="nt:unstructured"
                                                                                        text="Date"
                                                                                        value="date"/>
                                                                                <buttonList
                                                                                        jcr:primaryType="nt:unstructured"
                                                                                        text="Buttons List"
                                                                                        value="buttonList"/>
                                                                        </items>
                                                                </columnType>
                                                                <columnType
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        required="{Boolean}true"
                                                                        fieldLabel="Column Type"
                                                                        name="./columnKey"/>
                                                                <sortable
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                                        name="./sortable"
                                                                        text="Sortable"
                                                                        uncheckedValue="false"
                                                                        value="{Boolean}true"/>
                                                                <sortable-typehint
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                                                        name="./sortable@TypeHint"
                                                                        value="Boolean"/>
                                                            </items>
                                                        </column>
                                                    </items>
                                                </field>
                                            </actions>
                                            <detailUrl
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                    fieldLabel="Details URL"
                                                    jcr:primaryType="nt:unstructured"
                                                    rootPath="/content/techdata"
                                                    name="./detailUrl"/>
                                            <detailUrlSuffix
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Details URL Id Suffix"
                                                    jcr:primaryType="nt:unstructured"
                                                    rootPath="/content/techdata"
                                                    name="./detailUrlSuffix"/>
                                        </items>
                                    </column>
                                </items>
                            </elements>
                        </items>
                    </tabs>
                </items>
            </column>
        </items>
    </content>
</jcr:root>
