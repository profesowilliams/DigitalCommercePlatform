<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Properties"
          sling:resourceType="cq/gui/components/authoring/dialog">
    <content
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <column
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <tabs
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/tabs"
                            maximized="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <properties
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Properties"
                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                    margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <label
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Label"
                                                    name="./label"/>
                                            <itemsPerPage
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Items Per Page"
                                                    name="./itemsPerPage"/>
                                            <configDetailUrl
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                    fieldLabel="Config Details URL"
                                                    jcr:primaryType="nt:unstructured"
                                                    rootPath="/content/techdata"
                                                    name="./configDetailUrl"/>
                                            <configDetailUrlSuffix
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Config Details URL Id Suffix"
                                                    jcr:primaryType="nt:unstructured"
                                                    rootPath="/content/techdata"
                                                    name="./configDetailUrlSuffix"/>
                                            <quoteDetailUrl
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                    fieldLabel="Quote Details URL"
                                                    jcr:primaryType="nt:unstructured"
                                                    rootPath="/content/techdata"
                                                    name="./quoteDetailUrl"/>
                                            <quoteDetailUrlSuffix
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Quote Details URL Id Suffix"
                                                    jcr:primaryType="nt:unstructured"
                                                    rootPath="/content/techdata"
                                                    name="./quoteDetailUrlSuffix"/>
                                            <multipleSPADealLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="SPA Deals Label for Multiple items"
                                                    name="./spaDealsIdLabel"/>
                                            <paginationStyle
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/radiogroup"
                                                    fieldLabel="Select Pagination Style"
                                                    name="./paginationStyle">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <pages
                                                            jcr:primaryType="nt:unstructured"
                                                            checked="{Boolean}true"
                                                            text="Pages"
                                                            value="pages"/>
                                                    <scroll
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Scroll"
                                                            value="scroll"/>
                                                    <none
                                                            jcr:primaryType="nt:unstructured"
                                                            text="None"
                                                            value="none"/>
                                                </items>
                                            </paginationStyle>
                                            <defaultSortingColumnKey
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="/libs/granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Default Sorting Column Key"
                                                    fieldLabel="Default Sorting Column Key"
                                                    name="./defaultSortingColumnKey"/>
                                            <defaultSortingDirection
                                                    jcr:primaryType="nt:unstructured"
                                                    jcr:title="Sorting Direction"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/radiogroup"
                                                    name="./defaultSortingDirection"
                                                    margin="{Boolean}true">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <asc
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Ascending Order"
                                                            value="asc"/>
                                                    <desc
                                                            jcr:primaryType="nt:unstructured"
                                                            checked="{Boolean}true"
                                                            text="Descending Order"
                                                            value="desc"/>
                                                </items>
                                            </defaultSortingDirection>
                                        </items>
                                    </column>
                                </items>
                            </properties>
                            <elements
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Items"
                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                    margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <actions
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                    composite="{Boolean}true"
                                                    fieldDescription="Click 'Add' to add labels">
                                                <field
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                                        name="./columnList">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <column
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <columnLabel
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        required="{Boolean}true"
                                                                        fieldDescription="Enter column label"
                                                                        fieldLabel="Column Label"
                                                                        name="./columnLabel"/>
                                                                <columnKey
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        required="{Boolean}true"
                                                                        fieldDescription="Enter Column key for the column"
                                                                        fieldLabel="Column Key"
                                                                        name="./columnKey"/>
                                                                <sortable
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                                        name="./sortable"
                                                                        text="Sortable"
                                                                        uncheckedValue="false"
                                                                        value="{Boolean}true"/>
                                                                <sortable-typehint
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                                                        name="./sortable@TypeHint"
                                                                        value="Boolean"/>
                                                            </items>
                                                        </column>
                                                    </items>
                                                </field>
                                            </actions>
                                        </items>
                                    </column>
                                </items>
                            </elements>



                            <modalLabels
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Modal"
                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <title
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Enter Title"
                                            fieldLabel="Modal Title"
                                            name="./title"/>
                                    <buttonLabel
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Enter Button Label"
                                            fieldLabel="Button Label"
                                            name="./buttonLabel"/>
                                    <buttonIcon
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Enter Button Icon"
                                            fieldLabel="Button Icon"
                                            name="./buttonIcon"/>
                                    <content
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                            fieldDescription="Enter Content"
                                            fieldLabel="Content"
                                            name="./content"/>
                                </items>
                            </modalLabels>
                            <statusLabelsList
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Status Label Tab"
                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                    margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <actions
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                    composite="{Boolean}true"
                                                    fieldDescription="Click 'Add' to add label key and label value">
                                                <field
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                                        name="./statusLabelsList">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <column
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <labelKey
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        required="{Boolean}true"
                                                                        fieldLabel="Label Key"
                                                                        name="./labelKey"/>
                                                                <labelValue
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        required="{Boolean}true"
                                                                        fieldLabel="label Value"
                                                                        name="./labelValue"/>
                                                                <labelDescription
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldLabel="label Description"
                                                                        name="./labelDescription"/>
                                                                <buttonIcon
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldLabel="Button Icon"
                                                                        name="./buttonIcon"/>
                                                                <buttonLabel
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldLabel="Button Label"
                                                                        name="./buttonLabel"/>
                                                            </items>
                                                        </column>
                                                    </items>
                                                </field>
                                            </actions>
                                        </items>
                                    </column>
                                </items>
                            </statusLabelsList>



                            <searchCriteria
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Search Tab"
                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                    margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <searchTitle
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Enter Title"
                                                    fieldLabel="Search Title"
                                                    name="./searchTitle"/>
                                            <searchButtonLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Enter Label for Search Button"
                                                    fieldLabel="Search Button Label"
                                                    name="./searchButtonLabel"/>
                                            <clearButtonLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Enter Clear Button Label"
                                                    fieldLabel="Clear Button Label"
                                                    name="./clearButtonLabel"/>
                                            <inputPlaceholder
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Enter Input"
                                                    fieldLabel="Input Placeholder"
                                                    name="./inputPlaceholder"/>
                                            <keywordDropdownLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Label"
                                                    name="./keywordDropdownLabel"/>
                                            <actions
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                    composite="{Boolean}true"
                                                    fieldDescription="Click 'Add' to add key and value for drop down">
                                                <field
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                                        name="./keywordList">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <column
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <key
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        required="{Boolean}true"
                                                                        fieldDescription="Enter Key"
                                                                        fieldLabel="Key"
                                                                        name="./key"/>
                                                                <labelValue
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        required="{Boolean}true"
                                                                        fieldDescription="Enter Value"
                                                                        fieldLabel="Value"
                                                                        name="./value"/>
                                                            </items>
                                                        </column>
                                                    </items>
                                                </field>
                                            </actions>
                                            <configurationTypesDropdownLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Configuration Types Label"
                                                    name="./configurationTypesDropdownLabel"/>
                                            <configurationTypes
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                    composite="{Boolean}true"
                                                    fieldDescription="Click 'Add' to add key and value for drop down">
                                                <field
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                                        name="./configurationTypesList">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <column
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <key
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldDescription="Enter Key"
                                                                        fieldLabel="Configuration Types Key"
                                                                        name="./configurationTypesKey"/>
                                                                <labelValue
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldDescription="Enter Value"
                                                                        fieldLabel="Configuration Types Value"
                                                                        name="./configurationTypesValue"/>
                                                            </items>
                                                        </column>
                                                    </items>
                                                </field>
                                            </configurationTypes>
                                            <fromLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Enter From Label"
                                                    fieldLabel="From Label"
                                                    name="./fromLabel"/>
                                            <toLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Enter To Label"
                                                    fieldLabel="To Label"
                                                    name="./toLabel"/>
                                            <datePlaceholder
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Enter Date Placeholder"
                                                    fieldLabel="Date Placeholder"
                                                    name="./datePlaceholder"/>
                                        </items>
                                    </column>
                                </items>
                            </searchCriteria>
                        </items>
                    </tabs>
                </items>
            </column>
        </items>
    </content>
</jcr:root>
