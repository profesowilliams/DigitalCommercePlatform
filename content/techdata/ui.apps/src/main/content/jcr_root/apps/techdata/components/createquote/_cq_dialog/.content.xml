<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
		  jcr:primaryType="nt:unstructured"
		  jcr:title="Properties"
		  sling:resourceType="cq/gui/components/authoring/dialog">
	<content
			jcr:primaryType="nt:unstructured"
			sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
		<items jcr:primaryType="nt:unstructured">
			<column
					jcr:primaryType="nt:unstructured"
					sling:resourceType="granite/ui/components/coral/foundation/container">
				<items jcr:primaryType="nt:unstructured">
					<label
							jcr:primaryType="nt:unstructured"
							sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
							fieldLabel="Label"
							name="./label"/>
					<buttonTitle
							jcr:primaryType="nt:unstructured"
							sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
							fieldLabel="Button Label"
							name="./buttonTitle"/>
					<buttonTitleInProgress
							jcr:primaryType="nt:unstructured"
							sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
							fieldLabel="Action In progress Button Label"
							name="./buttonTitleInProgress"/>
					<createQuoteInProgress
							jcr:primaryType="nt:unstructured"
							sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
							fieldLabel="InProgress Message For Create Quote"
							name="./createQuoteInProgress"/>
					<createQuoteError
							jcr:primaryType="nt:unstructured"
							sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
							fieldLabel="Error Message For Create Quote"
							name="./createQuoteError"/>
					<quotePreviewUrl
							jcr:primaryType="nt:unstructured"
							sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
							fieldLabel="quote Preview Url"
							name="./quotePreviewUrl"/>
					<optionsList
							jcr:primaryType="nt:unstructured"
							sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
							composite="{Boolean}true"
							fieldDescription="Click 'Add' to add options list"
							fieldLabel="Options List">
						<field
								jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/foundation/form/fieldset"
								name="./optionsList">
							<items jcr:primaryType="nt:unstructured">
								<column
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/container">
									<items jcr:primaryType="nt:unstructured">
										<label
												jcr:primaryType="nt:unstructured"
												sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
												fieldDescription="Enter option label"
												fieldLabel="Option Label"
												name="./label"/>
										<key
												jcr:primaryType="nt:unstructured"
												sling:resourceType="granite/ui/components/coral/foundation/form/select"
												emptyText="Select a key"
												fieldLabel="Select Option Key"
												name="./key"
												required="{Boolean}true">
											<items jcr:primaryType="nt:unstructured">
												<active
														jcr:primaryType="nt:unstructured"
														text="active"
														value="active"/>
												<saved
														jcr:primaryType="nt:unstructured"
														text="saved"
														value="saved"/>
												<estimate
														jcr:primaryType="nt:unstructured"
														text="estimate"
														value="estimate"/>
											</items>
										</key>
										<title
												jcr:primaryType="nt:unstructured"
												sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
												fieldDescription="Enter Option Title"
												fieldLabel="Option Title"
												name="./title"/>
										<dropdownPlaceholder
												jcr:primaryType="nt:unstructured"
												sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
												fieldDescription="Enter Dropdown Placeholder"
												fieldLabel="Dropdown Placeholder"
												name="./dropdownPlaceholder"/>
										<textPlaceholder
												jcr:primaryType="nt:unstructured"
												sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
												fieldDescription="Enter Text Placeholder "
												fieldLabel="Text Placeholder "
												name="./textPlaceholder"/>
										<manuallyTypedError
												jcr:primaryType="nt:unstructured"
												sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
												fieldLabel="Enter Custom Error Message"
												name="./manuallyTypedError"/>
									</items>
								</column>
							</items>
						</field>
					</optionsList>
				</items>
			</column>
		</items>
	</content>
</jcr:root>
