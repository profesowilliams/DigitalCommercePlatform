<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Quote Detail"
    sling:resourceType="cq/gui/components/authoring/dialog"
    helpPath="https://www.adobe.com/go/aem_cmp_image_v2">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
               
                    <subheader
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Quote Subheader"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <subheaderLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Header Label"
                                                    name="./subheaderLabel"/>
                                            <subTotalLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Add subtotal label"
                                                    fieldLabel="Sub Total Label"
                                                    name="./subTotalLabel"/>
                                            <confirmButtonLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Confirm Button Label"
                                                    fieldLabel="Confirm Button Label"
                                                    name="./confirmButtonLabel"/>
                                            <makeChoiceLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Default Choice Label"
                                                    fieldLabel="Default Choice Label"
                                                    name="./makeChoiceLabel"/>
                                            <continueStandardPriceLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Continue With Standard Price Text"
                                                    fieldLabel="Continue With Standard Price Text"
                                                    name="./continueStandardPriceLabel"/>
                                            <createdDateLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Created Date Label"
                                                    fieldLabel="Created Date Label"
                                                    name="./createdDateLabel"/>
                                            <expiresDateLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Expires date label"
                                                    fieldLabel="Expires date label"
                                                    name="./expiresDateLabel"/>
                                        
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </subheader>
                    <AddressInfo
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Address Information"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <resellerContactLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Reseller Contact Label"
                                                fieldLabel="Reseller Contact Label"
                                                name="./resellerContactLabel"/>
                                            <endUserContactLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="End-user Contact Label"
                                                    fieldLabel="End-user Contact Label"
                                                    name="./endUserContactLabel"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </AddressInfo>
                    <SubTotal
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Sub Total"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <subtotalLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Sub-total Contact Label"
                                    fieldLabel="Sub-total Label"
                                    name="./subtotalLabel"/>
                        </items>
                    </SubTotal>
                    <PDF
                            jcr:primaryType="nt:unstructured"
                            jcr:title="PDF Configuration"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <logoUrl
                                    jcr:primaryType="nt:unstructured"
                                    path="/mnt/overlay/core/wcm/components/image/v2/image/cq:dialog/content/items/tabs/items/asset/items/columns/items/column/items/file"
                                    sling:resourceType="granite/ui/components/coral/foundation/include"/>
                            <downloadLinkText
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Text for Download Link"
                                    fieldLabel="Download Link Text"
                                    value="Download PDF"
                                    name="./downloadLinkText"/>
                            <pdfFileName
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Filename to be given for the downloaded file"
                                    fieldLabel="Download File Name"
                                    value="Download PDF"
                                    name="./fileName"/>
                        </items>
                    </PDF>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
