<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Quote Preview Properties"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <column
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <tabs
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/tabs"
                        maximized="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <properties
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Properties"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <headerLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Header Label"
                                                name="./headerLabel"/>
                                            <subtotalLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Sub Total Label"
                                                fieldLabel="Sub Total Label"
                                                name="./subtotalLabel"/>
                                            <confirmButtonLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Confirm Button Label"
                                                fieldLabel="Confirm Button Label"
                                                name="./confirmButtonLabel"/>
                                            <defaultChoiceLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Default Choice Label"
                                                fieldLabel="Default Choice Label"
                                                name="./defaultChoiceLabel"/>
                                            <continueWithStandardPriceLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Continue With Standard Price Link"
                                                fieldLabel="Continue With Standard Price Text"
                                                name="./continueWithStandardPriceLabel"/>
                                        </items>
                                    </column>
                                </items>
                            </properties>
                            <infoTab
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Information Tab"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <yourCompanyHeaderLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Your Company Header Label"
                                                fieldLabel="Your Company Header Label"
                                                name="./yourCompanyHeaderLabel"/>
                                            <endUserHeaderLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter End User Header Label"
                                                fieldLabel="End User Header Label"
                                                name="./endUserHeaderLabel"/>
                                            <generalHeaderLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter General Header Label"
                                                fieldLabel="General Header Label"
                                                name="./generalHeaderLabel"/>
                                            <emailLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Email Label"
                                                fieldLabel="Email Label"
                                                name="./emailLabel"/>
                                            <phoneLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Phone Label"
                                                fieldLabel="Phone Label"
                                                name="./phoneLabel"/>
                                            <sourceLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Source Label"
                                                fieldLabel="Source Label"
                                                name="./sourceLabel"/>
                                            <tierLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Tier Label"
                                                fieldLabel="Tier Label"
                                                name="./tierLabel"/>
                                            <referenceLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Reference Label"
                                                fieldLabel="Reference Label"
                                                name="./referenceLabel"/>
                                            <dealLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Deal Label"
                                                fieldLabel="Deal Label"
                                                name="./dealLabel"/>
                                        </items>
                                    </column>
                                </items>
                            </infoTab>
                            <elements
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Product Lines"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <label
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Gird Label"
                                                fieldLabel="Grid Label"
                                                name="./label"/>
                                            <expandAllLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Expand All Label"
                                                fieldLabel="Expand All Label"
                                                name="./expandAllLabel"/>
                                            <collapseAllLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Collapse All Label"
                                                fieldLabel="Collapse All Label"
                                                name="./collapseAllLabel"/>
                                            <startDateLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Start Date Label"
                                                fieldLabel="Start Date Label"
                                                name="./startDateLabel"/>
                                            <autoRenewLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Auto Renew Label"
                                                fieldLabel="Auto Renew Label"
                                                name="./autoRenewLabel"/>
                                            <durationLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Duration Label"
                                                fieldLabel="Duration Label"
                                                name="./durationLabel"/>
                                            <billingLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Billing Label"
                                                fieldLabel="Billing Label"
                                                name="./billingLabel"/>
                                            <yesLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Yes Label"
                                                fieldLabel="Yes Label"
                                                name="./yesLabel"/>
                                            <noLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter No Label"
                                                fieldLabel="No Label"
                                                name="./noLabel"/>
                                            <actions
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                composite="{Boolean}true"
                                                fieldDescription="Click 'Add' to add labels">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                                    name="./columnList">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <column
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <columnLabel
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldDescription="Enter column label"
                                                                    fieldLabel="Column Label"
                                                                    name="./columnLabel"/>
                                                                <columnKey
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldDescription="Enter Column key for the column"
                                                                    fieldLabel="Column Key"
                                                                    name="./columnKey"/>
                                                                <sortable
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                                    name="./sortable"
                                                                    text="Sortable"
                                                                    uncheckedValue="false"
                                                                    value="{Boolean}true"/>
                                                                <sortable-typehint
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                                                    name="./sortable@TypeHint"
                                                                    value="Boolean"/>
                                                            </items>
                                                        </column>
                                                    </items>
                                                </field>
                                            </actions>
                                        </items>
                                    </column>
                                </items>
                            </elements>
                            <noteTab
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Note Tab"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <noteLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Note Label"
                                                fieldLabel="Note Label"
                                                name="./noteLabel"/>
                                            <setupRequiredLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="/libs/granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Setup Required Label"
                                                fieldLabel="Setup Required Label"
                                                name="./setupRequiredLabel"/>
                                            <attachmentRequiredLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="/libs/granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Attachment Required Label"
                                                fieldLabel="Attachment Required Label"
                                                name="./attachmentRequiredLabel"/>
                                        </items>
                                    </column>
                                </items>
                            </noteTab>
                        </items>
                    </tabs>
                </items>
            </column>
        </items>
    </content>
</jcr:root>
