<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Quote Preview Properties"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <column
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <tabs
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/tabs"
                        maximized="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <properties
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Properties"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <headerLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Header Label"
                                                name="./headerLabel"/>
                                            <successRedirectPage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Succes Redirect Page"
                                                name="./successRedirectPage"/>
                                            <subtotalLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Sub Total Label"
                                                fieldLabel="Sub Total Label"
                                                name="./subtotalLabel"/>
                                            <confirmButtonLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Confirm Button Label"
                                                fieldLabel="Confirm Button Label"
                                                name="./confirmButtonLabel"/>
                                            <defaultChoiceLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Default Choice Label"
                                                fieldLabel="Default Choice Label"
                                                name="./defaultChoiceLabel"/>
                                            <continueWithStandardPriceLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Continue With Standard Price Link"
                                                fieldLabel="Continue With Standard Price Text"
                                                name="./continueWithStandardPriceLabel"/>
                                        </items>
                                    </column>
                                </items>
                            </properties>
                            <infoTab
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Information Tab"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <yourCompanyHeaderLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Your Company Header Label"
                                                fieldLabel="Your Company Header Label"
                                                name="./yourCompanyHeaderLabel"/>
                                            <endUserHeaderLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter End User Header Label"
                                                fieldLabel="End User Header Label"
                                                name="./endUserHeaderLabel"/>
                                            <emailLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Email Label"
                                                fieldLabel="Email Label"
                                                name="./emailLabel"/>
                                            <phoneLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Phone Label"
                                                fieldLabel="Phone Label"
                                                name="./phoneLabel"/>
                                            <companyLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Company Label"
                                                fieldLabel="Company Label"
                                                name="./companyLabel"/>
                                            <nameLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Name Label"
                                                fieldLabel="Name Label"
                                                name="./nameLabel"/>
                                            <addressLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Address Label"
                                                fieldLabel="Address Label"
                                                name="./addressLabel"/>
                                            <cityLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter City Label"
                                                fieldLabel="City Label"
                                                name="./cityLabel"/>
                                            <stateLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter State Label"
                                                fieldLabel="State Label"
                                                name="./stateLabel"/>
                                            <zipLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Zip Label"
                                                fieldLabel="Zip Label"
                                                name="./zipLabel"/>
                                            <countryLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Country Label"
                                                fieldLabel="Country Label"
                                                name="./countryLabel"/>
                                            <userInfoIndustlabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="User Information Industries label"
                                                fieldLabel="User Information Industries label"
                                                name="./userInfoIndustlabel"/>
                                            <userInfoNameLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="User Information Name label"
                                                fieldLabel="User Information  Name label"
                                                name="./userInfoNamelabel"/>
                                            <userInfoaddressLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="User Information  Address Label"
                                                fieldLabel="User Information  Address Label"
                                                name="./userInfoAddressLabel"/>
                                            <userInfoCitylabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="User Information City  label"
                                                fieldLabel="User Information City label"
                                                name="./userInfoCitylabel"/>
                                            <userInfoStatelabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="User Information State  label"
                                                fieldLabel="User Information State label"
                                                name="./userInfoStatelabel"/>
                                            <userInfoZiplabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="User Information Zip  label"
                                                fieldLabel="User Information Zip  label"
                                                name="./userInfoZiplabel"/>
                                            <userInfoCountryLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="User Information Country  label"
                                                fieldLabel="User Information Country  label"
                                                name="./userInfoCountryLabel"/>
                                            <userInfoEmailLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="User Information Email label"
                                                fieldLabel="User Information Email label"
                                                name="./userInfoEmailLabel"/>
                                            <userInfoPhoneLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="User Information Phone label"
                                                fieldLabel="User Information Phone label"
                                                name="./userInfoPhoneLabel"/>
                                            <generalInformation
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset"
                                                jcr:title="General Information">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <generalHeaderLabel
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        required="{Boolean}true"
                                                        fieldLabel="General Header Label"
                                                        name="./generalHeaderLabel"/>
                                                    <sourceLabel
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Source Label"
                                                        name="./sourceLabel"/>
                                                    <referenceLabel
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Reference Label"
                                                        name="./referenceLabel"/>
                                                    <referenceMaxCharacterError
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldDescription="Maximum amount of characters exceeded"
                                                        fieldLabel="Maximum amount of characters exceeded"
                                                        name="./referenceMaxCharacterError"/>  
                                                    <dealLabel
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Deal Label"
                                                        name="./dealLabel"/>
                                                    <tierLabel
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Tier Label"
                                                        name="./tierLabel"/>
                                                    <searchDealsPlaceholder
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Search Deals Placeholder"
                                                        name="./searchDealsPlaceholder"/>
                                                    <searchingDealsLabel
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Searching Deals Label"
                                                        name="./searchingDealsLabel"/>
                                                    <dealsFoundLabel
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Deals Found Label"
                                                        name="./dealsFoundLabel"/>
                                                    <noDealsFoundLabel
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="No Deals Found Label"
                                                        name="./noDealsFoundLabel"/>
                                                    <bidLabel
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Bid Label"
                                                        name="./bidLabel"/>
                                                    <versionLabel
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Version Label"
                                                        name="./versionLabel"/>
                                                    <spaIdLabel
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="SPA Id Label"
                                                        name="./spaIdLabel"/>
                                                    <endUserNameLabel
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="End User Label"
                                                        name="./endUserNameLabel"/>
                                                    <errorGettingDealsLabel
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Error Getting Deals Label"
                                                        name="./errorGettingDealsLabel"/>
                                                    <dealsPricingNote
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Deals Pricing Note"
                                                        name="./dealsPricingNote"/>
                                                    <submitLabel
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldLabel="Submit Button Label"
                                                            name="./submitLabel"/>
                                                    <cancelLabel
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldLabel="Cancel Button Label"
                                                            name="./cancelLabel"/>
                                                </items>
                                            </generalInformation>
                                        </items>
                                    </column>
                                </items>
                            </infoTab>
                            <elements
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Product Lines"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <label
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Gird Label"
                                                fieldLabel="Grid Label"
                                                name="./label"/>
                                            <expandAllLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Expand All Label"
                                                fieldLabel="Expand All Label"
                                                name="./expandAllLabel"/>
                                            <collapseAllLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Collapse All Label"
                                                fieldLabel="Collapse All Label"
                                                name="./collapseAllLabel"/>
                                            <startDateLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Start Date Label"
                                                fieldLabel="Start Date Label"
                                                name="./startDateLabel"/>
                                            <autoRenewLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Auto Renew Label"
                                                fieldLabel="Auto Renew Label"
                                                name="./autoRenewLabel"/>
                                            <durationLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Duration Label"
                                                fieldLabel="Duration Label"
                                                name="./durationLabel"/>
                                            <billingLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Billing Label"
                                                fieldLabel="Billing Label"
                                                name="./billingLabel"/>
                                            <yesLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Yes Label"
                                                fieldLabel="Yes Label"
                                                name="./yesLabel"/>
                                            <noLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter No Label"
                                                fieldLabel="No Label"
                                                name="./noLabel"/>
                                            <actions
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                composite="{Boolean}true"
                                                fieldDescription="Click 'Add' to add labels">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                                    name="./columnList">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <column
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <columnLabel
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldDescription="Enter column label"
                                                                    fieldLabel="Column Label"
                                                                    name="./columnLabel"/>
                                                                <columnKey
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldDescription="Enter Column key for the column"
                                                                    fieldLabel="Column Key"
                                                                    name="./columnKey"/>
                                                                <sortable
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                                    name="./sortable"
                                                                    text="Sortable"
                                                                    uncheckedValue="false"
                                                                    value="{Boolean}true"/>
                                                                <sortable-typehint
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                                                    name="./sortable@TypeHint"
                                                                    value="Boolean"/>
                                                            </items>
                                                        </column>
                                                    </items>
                                                </field>
                                            </actions>
                                        </items>
                                    </column>
                                </items>
                            </elements>
                            <noteTab
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Note Tab"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <noteLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Note Label"
                                                fieldLabel="Note Label"
                                                name="./noteLabel"/>
                                            <setupRequiredLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="/libs/granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Setup Required Label"
                                                fieldLabel="Setup Required Label"
                                                name="./setupRequiredLabel"/>
                                            <attachmentRequiredLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="/libs/granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Attachment Required Label"
                                                fieldLabel="Attachment Required Label"
                                                name="./attachmentRequiredLabel"/>
                                        </items>
                                    </column>
                                </items>
                            </noteTab>
                            <modalConfigTab
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Modal Configuration"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                margin="{Boolean}true">

                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <avgValue
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter AVG Label"
                                                fieldLabel="Enter AVG Label"
                                                name="./avgValue"/>
                                            <techDataValue
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="/libs/granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter TechData Label"
                                                fieldLabel="Setup TechData Label"
                                                name="./techDataValue"/>
                                            <title
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="/libs/granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Title Of The Modal"
                                                fieldLabel="Setup Title Of The Modal"
                                                name="./title"/>
                                        </items>
                                    </column>
                                </items>

                            </modalConfigTab>
                        </items>
                    </tabs>
                </items>
            </column>
        </items>
    </content>
</jcr:root>
