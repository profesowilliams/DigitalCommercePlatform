<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Search Results"
    sling:resourceType="cq/gui/components/authoring/dialog">
	<content
			jcr:primaryType="nt:unstructured"
			sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
		<items jcr:primaryType="nt:unstructured">
			<column
					jcr:primaryType="nt:unstructured"
					sling:resourceType="granite/ui/components/coral/foundation/container">
				<items jcr:primaryType="nt:unstructured">
					<label
						jcr:primaryType="nt:unstructured"
						sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
						fieldLabel="Label"
						name="./label"/>
					<baseHref
						jcr:primaryType="nt:unstructured"
						sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
						fieldLabel=" UI Base Url"
						name="./baseHref"/>
					<loginPath
						jcr:primaryType="nt:unstructured"
						sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
						fieldLabel=" Login path"
						name="./loginPath"/>
					<loginRedirectUrlQueryParameter
						jcr:primaryType="nt:unstructured"
						sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
						fieldLabel=" Login redirect Url"
						name="./loginRedirectUrlQueryParameter"/>
					<uiSearchBaseUrl
						jcr:primaryType="nt:unstructured"
						sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
						fieldLabel=" UI Search Base Url"
						name="./uiSearchBaseUrl"/>
					<uiBrowseBaseUrl
						jcr:primaryType="nt:unstructured"
						sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
						fieldLabel=" UI Browse Base Url"
						name="./uiBrowseBaseUrl"/>
					<uiLocalizeBaseUrl
						jcr:primaryType="nt:unstructured"
						sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
						fieldLabel=" UI Localize Base Url"
						name="./uiLocalizeBaseUrl"/>
				</items>
			</column>
		</items>
	</content>
</jcr:root>