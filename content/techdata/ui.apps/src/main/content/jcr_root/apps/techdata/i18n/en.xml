<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:mix="http://www.jcp.org/jcr/mix/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:language="en"
    jcr:mixinTypes="[mix:language]"
    jcr:primaryType="nt:folder">

    <techdata.validation.message.invalidEmail jcr:mixinTypes="[sling:Message]" jcr:primaryType="nt:folder" sling:message="Enter a valid email address. For example, name@email.com"/>
    <techdata.validation.message.requiredField jcr:mixinTypes="[sling:Message]" jcr:primaryType="nt:folder" sling:message="This is a required field."/>
    <techdata.validation.message.requiredMaxLengthField jcr:mixinTypes="[sling:Message]" jcr:primaryType="nt:folder" sling:message="This is a required field, max {max-length} characters."/>
    <techdata.validation.message.maxLengthField jcr:mixinTypes="[sling:Message]" jcr:primaryType="nt:folder" sling:message="Max {max-length} characters."/>
    
    <techdata.buttons.label.save jcr:mixinTypes="[sling:Message]" jcr:primaryType="nt:folder" sling:message="Save"/>
    <techdata.buttons.label.cancel jcr:mixinTypes="[sling:Message]" jcr:primaryType="nt:folder" sling:message="Cancel"/>
    <techdata.buttons.label.edit jcr:mixinTypes="[sling:Message]" jcr:primaryType="nt:folder" sling:message="Edit"/>

    <techdata.buttons.label.cancelDialogNo jcr:mixinTypes="[sling:Message]" jcr:primaryType="nt:folder" sling:message="No, continue editing"/>
    <techdata.buttons.label.cancelDialogYes jcr:mixinTypes="[sling:Message]" jcr:primaryType="nt:folder" sling:message="Yes, cancel"/>

    <techdata.quotes.placeholder.createfrom jcr:mixinTypes="[sling:Message]" jcr:primaryType="nt:folder" sling:message="Create From"/>
    <techdata.quotes.placeholder.selectpricingtype jcr:mixinTypes="[sling:Message]" jcr:primaryType="nt:folder" sling:message="Select pricing type"/>
	<techdata.quotes.message.uanError jcr:mixinTypes="[sling:Message]" jcr:primaryType="nt:folder" sling:message="You have reached your qty limit on the below products. Please contact your Cisco Sales Rep for further assistance."/>

	<techdata.grids.message.noRows jcr:mixinTypes="[sling:Message]" jcr:primaryType="nt:folder" sling:message="No rows found"/>
    <techdata.grids.message.error.401 jcr:mixinTypes="[sling:Message]" jcr:primaryType="nt:folder" sling:message="You need to login"/>
    <techdata.grids.message.error.403 jcr:mixinTypes="[sling:Message]" jcr:primaryType="nt:folder" sling:message="No permissions"/>
    <techdata.grids.message.error.404 jcr:mixinTypes="[sling:Message]" jcr:primaryType="nt:folder" sling:message="Not found"/>
    <techdata.grids.message.error.408 jcr:mixinTypes="[sling:Message]" jcr:primaryType="nt:folder" sling:message="Timeout"/>
    <techdata.grids.message.error.500 jcr:mixinTypes="[sling:Message]" jcr:primaryType="nt:folder" sling:message="Unknown error"/>
</jcr:root>
