<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Order Detail"
    sling:resourceType="cq/gui/components/authoring/dialog"
    helpPath="https://www.adobe.com/go/aem_cmp_image_v2">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <orderHeader
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Order Label Configuration"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <orderLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Order Label"
                                                    fieldLabel="Order Label"
                                                    name="./orderLabel"/>
                                            <orderDateLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Order Date Label"
                                                    fieldLabel="Order Date Label"
                                                    name="./orderDateLabel"/>
                                            <purchaseOrderLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="PO Label"
                                                    fieldLabel="Purchase Order Label"
                                                    name="./purchaseOrderLabel"/>
                                        </items>
                                    </column1>
                                    <column2
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <phoneNumberLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Phone Number Label"
                                                    fieldLabel="Phone Number Label"
                                                    name="./phoneNumberLabel"/>
                                            <exportCSVLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Export to CSV Label"
                                                    fieldLabel="Export to CSV Label"
                                                    name="./exportCSVLabel"/>
                                            <serialCellNotFoundMessage
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Serial Not Found Label"
                                                    fieldLabel="Serial Not Found Label"
                                                    name="./serialCellNotFoundMessage"/>
                                            <serialCellLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Serial Cell Label"
                                                    fieldLabel="Serial Cell Label"
                                                    name="./serialCellLabel"/>
                                            <serialModal
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Serial Modal Label"
                                                    fieldLabel="Serial Modal Label"
                                                    name="./serialModal"/>

                                        </items>
                                    </column2>
                                </items>
                            </columns>
                        </items>
                    </orderHeader>
                    <columnConfiguration
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/include"
                            jcr:title="Column Configuration"
                            path="/mnt/overlay/techdata/components/ordersgrid/cq:dialog/content/items/column/items/tabs/items/elements"
                    />
                    <iconConfiguration
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/include"
                            path="/mnt/overlay/techdata/components/ordersgrid/cq:dialog/content/items/column/items/tabs/items/tab3"
                    />
                    <trackingInfo
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/include"
                            path="/mnt/overlay/techdata/components/ordersgrid/cq:dialog/content/items/column/items/tabs/items/trackingInfo"
                    />
                    <searchCriteria
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Search Configuration"
                            sling:resourceType="granite/ui/components/coral/foundation/include"
                            path="/mnt/overlay/techdata/components/ordersgrid/cq:dialog/content/items/column/items/tabs/items/searchCriteria"
                    >
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                            >
                                <items jcr:primaryType="nt:unstructured">
                                    <clearButtonLabel
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Enter Clear Button Label"
                                            fieldLabel="Clear Button Label"
                                            name="./clearButtonLabel"/>
                                    <inputPlaceholder
                                            jcr:primaryType="nt:unstructured"
                                            sling:hideResource="{Boolean}true"/>
                                    <keywordDropdownLabel
                                            jcr:primaryType="nt:unstructured"
                                            sling:hideResource="{Boolean}true" />
                                    <actions
                                            jcr:primaryType="nt:unstructured"
                                            sling:hideResource="{Boolean}true" />
                                    <vendorDropdownLabel
                                            jcr:primaryType="nt:unstructured"
                                            sling:hideResource="{Boolean}true" />
                                    <vendorActions
                                            jcr:primaryType="nt:unstructured"
                                            sling:hideResource="{Boolean}true" />
                                    <methodDropdownLabel
                                            jcr:primaryType="nt:unstructured"
                                            sling:hideResource="{Boolean}true" />
                                    <methodActions
                                            jcr:primaryType="nt:unstructured"
                                            sling:hideResource="{Boolean}true" />
                                    <fromLabel
                                            jcr:primaryType="nt:unstructured"
                                            sling:hideResource="{Boolean}true"/>
                                    <toLabel
                                            jcr:primaryType="nt:unstructured"
                                            sling:hideResource="{Boolean}true" />
                                    <datePlaceholder
                                            jcr:primaryType="nt:unstructured"
                                            sling:hideResource="{Boolean}true"/>
                                </items>
                            </column>
                        </items>
                    </searchCriteria>
                    <orderInfo
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Order Information Configuration"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <tabs
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/tabs"
                                    maximized="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <statusInfo
                                            jcr:primaryType="nt:unstructured"
                                            jcr:title="Status Info"
                                            sling:resourceType="granite/ui/components/coral/foundation/container"
                                            margin="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
                                            <columns
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                            margin="{Boolean}true">
                                            <items jcr:primaryType="nt:unstructured">
                                                <column1
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <orderStatusLabel
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldDescription="Order Status Label"
                                                            fieldLabel="Order Status Label"
                                                            name="./orderStatusLabel"/>
                                                        <orderStatusItemsOpen
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldDescription="Order open Message"
                                                                fieldLabel="Order open  message"
                                                                name="./orderStatusItemsOpen"/>
                                                        <orderStatusItemsShipped
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldDescription="All Items Shipped Message"
                                                                fieldLabel="All Items Shipped Message"
                                                                name="./orderStatusItemsShipped"/>
                                                        <orderStatusItemsInProcess
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldDescription="All Items being Processed  Message"
                                                                fieldLabel="Items are being Processed  message"
                                                                name="./orderStatusItemsInProcess"/>
                                                        <orderStatusItemsInReview
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldDescription="Order In-review Message"
                                                                fieldLabel="Order In-review Message"
                                                                name="./orderStatusItemsInReview"/>
                                                        <orderStatusItemsCancelled
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldDescription="Order Cancelled Message"
                                                                fieldLabel="Order Cancelled Message"
                                                                name="./orderStatusItemsCancelled"/>
                                                    </items>
                                                </column1>
                                                <column2
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <orderStatusItemsOpenDescription
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldDescription="All Items Open Descriptive Message"
                                                                fieldLabel="All Items Open Descriptive Message"
                                                                name="./orderStatusItemsOpenDescription"/>
                                                        <orderStatusItemsShippedDescription
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldDescription="All Items Shipped Descriptive Message"
                                                                fieldLabel="All Items Shipped Descriptive Message"
                                                                name="./orderStatusItemsShippedDescription"/>
                                                        <orderStatusItemsInProcessDescription
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldDescription="All Items Shipped Message"
                                                                fieldLabel="Items are being Processed message"
                                                                name="./orderStatusItemsInProcessDescription"/>
                                                        <orderStatusItemsInReviewDescription
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldDescription="Order In-review Descriptive Message"
                                                                fieldLabel="Order In-review Descriptive Message"
                                                                name="./orderStatusItemsInReviewDescription"/>
                                                        <orderStatusItemsCancelledDescription
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldDescription="Order Cancelled Descriptive Message"
                                                                fieldLabel="Order Cancelled Descriptive Message"
                                                                name="./orderStatusItemsCancelledDescription"/>
                                                    </items>
                                                </column2>

                                            </items>
                                        </columns>
                                        </items>
                                    </statusInfo>
                                    <endUserInfo
                                            jcr:primaryType="nt:unstructured"
                                            jcr:title="End User and Ship To Info"
                                            sling:resourceType="granite/ui/components/coral/foundation/container"
                                            margin="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
                                            <columns
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                                    margin="{Boolean}true">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <column1
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <endUserLabel
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldDescription="End-user Card Label"
                                                                    fieldLabel="End-user Card Label"
                                                                    name="./endUserLabel"/>
                                                            <shipToLabel
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldDescription="Ship-To Card Label"
                                                                    fieldLabel="Ship-To Card Label"
                                                                    name="./shipToLabel"/>
                                                        </items>
                                                    </column1>
                                                    <column2
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <blindPackagingUsed
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldDescription="Blind packaging used Label"
                                                                    fieldLabel="Blind packaging used Label"
                                                                    name="./blindPackagingUsed"/>
                                                            <blindPackagingNotUsed
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldDescription="Blind packaging not used Label"
                                                                    fieldLabel="Blind packaging not used Label"
                                                                    name="./blindPackagingNotUsed"/>
                                                        </items>
                                                    </column2>

                                                </items>
                                            </columns>
                                        </items>
                                    </endUserInfo>
                                    <paymentInfo
                                            jcr:primaryType="nt:unstructured"
                                            jcr:title="Payment Info"
                                            sling:resourceType="granite/ui/components/coral/foundation/container"
                                            margin="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
                                            <columns
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                                    margin="{Boolean}true">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <column
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <paymentLabel
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldDescription="Payment Card Label"
                                                                    fieldLabel="Payment Card Label"
                                                                    name="./paymentLabel"/>
                                                            <subtotalLabelLabel
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldDescription="Sub-total Label"
                                                                    fieldLabel="Sub-total Label"
                                                                    name="./subTotalLabel"/>
                                                            <freightLabel
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldDescription="Freight Label"
                                                                    fieldLabel="Freight Label"
                                                                    name="./freightLabel"/>
                                                            <taxLabel
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldDescription="Tax Label"
                                                                    fieldLabel="Tax Label"
                                                                    name="./taxLabel"/>
                                                            <otherFeesLabel
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldDescription="Other Fees Label"
                                                                    fieldLabel="Other Fees Label"
                                                                    name="./otherFeesLabel"/>
                                                            <paymentTermsLabel
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldDescription="Payment Terms Label"
                                                                    fieldLabel="Payment Terms Label"
                                                                    name="./paymentTermsLabel"/>
                                                            <paymentTotalLabel
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldDescription="Payment Total Label"
                                                                    fieldLabel="Payment Total Label"
                                                                    name="./paymentTotalLabel"/>
                                                            <currencyLabel
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldDescription="Currency Label"
                                                                    fieldLabel="Currency Label"
                                                                    name="./currencyLabel"/>
                                                        </items>
                                                    </column>
                                                </items>
                                            </columns>
                                        </items>
                                    </paymentInfo>
                                </items>
                            </tabs>

                        </items>
                    </orderInfo>
                    <SubTotal
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Sub Total"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <subtotalLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Sub-total Contact Label"
                                    fieldLabel="Sub-total Label"
                                    name="./subtotalLabel"/>
                        </items>
                    </SubTotal>
                    <PDF
                            jcr:primaryType="nt:unstructured"
                            jcr:title="PDF Configuration"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <logoUrl
                                    jcr:primaryType="nt:unstructured"
                                    path="/mnt/overlay/core/wcm/components/image/v2/image/cq:dialog/content/items/tabs/items/asset/items/columns/items/column/items/file"
                                    sling:resourceType="granite/ui/components/coral/foundation/include"/>
                            <downloadLinkText
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Text for Download Link"
                                    fieldLabel="Download Link Text"
                                    value="Download PDF"
                                    name="./downloadLinkText"/>
                            <pdfFileName
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="Filename to be given for the downloaded file"
                                    fieldLabel="Download File Name"
                                    value="Download PDF"
                                    name="./fileName"/>
                            <downloadInvoicesEndpoint
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="URL for Download Link"
                                    fieldLabel="URL for Download Link"
                                    name="./downloadInvoicesEndpoint"/>
                        </items>
                    </PDF>
                      <exportColumnList
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Export Column List"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <columnList
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                        composite="{Boolean}true"
                                        fieldDescription="Click 'Add' to add key and value for drop down"
                                        fieldLabel="Column List">
                                        <field
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container"
                                            name="./exportColumnList">
                                            <items jcr:primaryType="nt:unstructured">
                                                <column
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <columnLabel
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldDescription="Column Label"
                                                            fieldLabel="Column Label"
                                                            name="./columnLabel"/>
                                                        <columnKey
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldDescription="Column key"
                                                            fieldLabel="Column key"
                                                            name="./columnKey"/>
                                                    </items>
                                                </column>
                                            </items>
                                        </field>
                                    </columnList>
                                </items>
                            </column>
                        </items>
                    </exportColumnList>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
